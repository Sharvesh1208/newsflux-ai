#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/news-scrapper2/node_modules/.pnpm/react-scripts@5.0.1_@babel+_bfc7e8ab0651ea5266e22e7b79f7e594/node_modules/react-scripts/bin/node_modules:/mnt/e/news-scrapper2/node_modules/.pnpm/react-scripts@5.0.1_@babel+_bfc7e8ab0651ea5266e22e7b79f7e594/node_modules/react-scripts/node_modules:/mnt/e/news-scrapper2/node_modules/.pnpm/react-scripts@5.0.1_@babel+_bfc7e8ab0651ea5266e22e7b79f7e594/node_modules:/mnt/e/news-scrapper2/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/news-scrapper2/node_modules/.pnpm/react-scripts@5.0.1_@babel+_bfc7e8ab0651ea5266e22e7b79f7e594/node_modules/react-scripts/bin/node_modules:/mnt/e/news-scrapper2/node_modules/.pnpm/react-scripts@5.0.1_@babel+_bfc7e8ab0651ea5266e22e7b79f7e594/node_modules/react-scripts/node_modules:/mnt/e/news-scrapper2/node_modules/.pnpm/react-scripts@5.0.1_@babel+_bfc7e8ab0651ea5266e22e7b79f7e594/node_modules:/mnt/e/news-scrapper2/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/react-scripts.js" "$@"
else
  exec node  "$basedir/../../bin/react-scripts.js" "$@"
fi
