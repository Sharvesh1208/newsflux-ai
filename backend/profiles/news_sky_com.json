{
  "base_url": "https://news.sky.com/world",
  "search_pattern": "https://news.sky.com/world/?s=corporate finance",
  "requires_js": false,
  "selectors": {
    "article_container": [
      "section.wrap",
      "section.ui-news-footer-section",
      "div.site-wrap"
    ],
    "headline": [
      "h1, h2, h3",
      "[class*=\"headline\"]",
      "[class*=\"title\"]",
      "a > h1, a > h2, a > h3"
    ],
    "link": {
      "selector": "a[href]",
      "filter": "article_links",
      "base_url": "https://news.sky.com/world"
    },
    "description": [
      "p",
      "[class*=\"description\"]",
      "[class*=\"excerpt\"]",
      "[class*=\"summary\"]",
      "[class*=\"snippet\"]"
    ]
  },
  "pagination": null
}