{
  "base_url": "https://www.theguardian.com/world",
  "search_pattern": "https://www.theguardian.com/world/search/corporate finance",
  "requires_js": false,
  "selectors": {
    "article_container": [
      "section.dcr-dqlcr0",
      "div.dcr-1h5w6j3",
      "div.dcr-l9k7rx"
    ],
    "headline": [
      "h3.card-headline",
      "h1, h2, h3",
      "[class*=\"headline\"]",
      "[class*=\"title\"]",
      "a > h1, a > h2, a > h3"
    ],
    "link": {
      "selector": "a[href]",
      "filter": "article_links",
      "base_url": "https://www.theguardian.com/world"
    },
    "description": [
      "p",
      "[class*=\"description\"]",
      "[class*=\"excerpt\"]",
      "[class*=\"summary\"]",
      "[class*=\"snippet\"]"
    ]
  },
  "pagination": null
}