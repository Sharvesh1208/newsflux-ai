{
  "base_url": "https://news.google.com/home",
  "search_pattern": "https://news.google.com/home/search?q=finance",
  "requires_js": false,
  "selectors": {
    "article_container": [
      "section.XhbDsd"
    ],
    "headline": [
      "h1, h2, h3",
      "[class*=\"headline\"]",
      "[class*=\"title\"]",
      "a > h1, a > h2, a > h3"
    ],
    "link": {
      "selector": "a[href]",
      "filter": "article_links",
      "base_url": "https://news.google.com/home"
    },
    "description": [
      "p",
      "[class*=\"description\"]",
      "[class*=\"excerpt\"]",
      "[class*=\"summary\"]",
      "[class*=\"snippet\"]"
    ]
  },
  "pagination": null
}