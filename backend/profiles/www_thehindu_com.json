{
  "base_url": "https://www.thehindu.com/",
  "search_pattern": "https://www.thehindu.com//search?q=corporate finance",
  "requires_js": true,
  "selectors": {
    "article_container": [
      "article",
      "div[class*=\"article\"]",
      "li"
    ],
    "headline": [
      "h1, h2, h3",
      "[class*=\"headline\"]",
      "[class*=\"title\"]",
      "a > h1, a > h2, a > h3"
    ],
    "link": {
      "selector": "a[href]",
      "filter": "article_links",
      "base_url": "https://www.thehindu.com/"
    },
    "description": [
      "p",
      "[class*=\"description\"]",
      "[class*=\"excerpt\"]",
      "[class*=\"summary\"]",
      "[class*=\"snippet\"]"
    ]
  },
  "pagination": null
}